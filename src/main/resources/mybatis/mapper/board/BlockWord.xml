<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sample.board.mysql.mapper.board.BlockWordMapper">
    <select id="getBlockWordList" resultType="com.sample.board.mysql.model.board.BlockWord">
        SELECT *
            FROM  sample_board.block_words
            WHERE status = 1;
    </select>
</mapper>